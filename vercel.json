{
  "version": 2,
  "name": "sg-bus-api",
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "build": {
    "env": {
      "DatamallAccountKey": "@datamall-account-key"
    }
  }
}
